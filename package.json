        "@types/jest": "24.0.19",
        "@types/puppeteer": "1.20.2",
        "chokidar": "^3.2.2",
        "concurrently": "^5.0.0",
        "glob": "^7.1.4",
        "highlight.js": "^9.15.10",
        "jest": "24.8.0",
        "jest-cli": "24.8.0",
        "markdown-it": "^10.0.0",
        "mem": "^5.1.1",
        "puppeteer": "1.20.0",
        "tslint": "^5.20.0",
        "tslint-ionic-rules": "^0.0.21",
        "xstate": "^4.6.7"
    },
