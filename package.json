{
    "name": "wcs-core",
    "version": "1.17.1",
    "description": "Web components SNCF",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/components.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/wcs/wcs.js",
    "files": [
        "dist/",
        "loader/",
        "schematics/"
    ],
    "scripts": {
        "build:stencil": "stencil build",
        "build:examples": "node build.js",
        "build:schematics": "node_modules/.bin/tsc -p schematics/tsconfig.json",
        "build": "node build.js && stencil build && npm run build:schematics",
        "watch:stencil": "stencil build --dev --serve --watch",
        "watch:examples": "node build.js --watch",
        "start": "node run.js",
        "test": "stencil test --spec --e2e",
        "test:watch": "stencil test --spec --e2e --watchAll",
        "update-version": "node scripts/update-version.js",
        "update-develop": "node scripts/update-develop-from-master.js"
    },
    "devDependencies": {
        "@angular/cli": "^10.1.1",
        "@material/ripple": "1.1.0",
        "@stencil/core": "^2.4.0",
        "@stencil/sass": "^1.4.1",
        "@types/dom-inputevent": "^1.0.5",
        "@types/glob": "^7.1.3",
        "@types/jest": "26.0.7",
        "@types/puppeteer": "^3.0.1",
        "chokidar": "^3.4.1",
        "concurrently": "^5.2.0",
        "glob": "^7.1.6",
        "highlight.js": "^10.1.2",
        "jest": "^26.1.0",
        "jest-cli": "^26.1.0",
        "markdown-it": "^11.0.0",
        "mem": "^6.1.0",
        "puppeteer": "^5.2.1",
        "rollup": "^2.23.0",
        "rollup-plugin-typescript": "^1.0.1",
        "tslib": "^2.0.0",
        "tslint": "^6.1.2",
        "tslint-ionic-rules": "^0.0.21",
        "typescript": "~3.9.7"
    },
    "dependencies": {
        "@angular-devkit/core": "^10.0.4",
        "@angular-devkit/schematics": "^10.0.4",
        "@angular/cdk": "^10.1.1",
        "@schematics/angular": "^10.0.4",
        "chalk": "^4.1.0",
        "lodash": "^4.17.21",
        "uuid": "^8.3.2",
        "xstate": "^4.11.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SNCFDevelopers/wcs-sandbox"
    },
    "author": "SNCF",
    "private": false,
    "license": "MIT",
    "schematics": "./schematics/collection.json"
}
